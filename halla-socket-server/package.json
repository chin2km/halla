{
  "name": "halla-socket-server",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git"
  },
  "author": "Chintu Kishen KM",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/src/index.js",
    "watch-node": "nodemon dist/src/index.js",
    "build-ts": "tsc",
    "watch": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "rabbit.js": "0.4.4",
    "ramda": "0.25.0",
    "socket.io": "2.0.4"
  },
  "devDependencies": {
    "@types/ramda": "0.25.18",
    "@types/rabbit.js": "0.4.31",
    "@types/socket.io": "1.4.31",
    "nodemon": "1.15.1",
    "ts-node": "5.0.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
