{
  "name": "halla-socket-server",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git"
  },
  "author": "Chintu Kishen KM",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "watch-node": "nodemon dist/src/index.js",
    "watch-all": "npm-run-all --parallel watch watch-node",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/src/index.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "rabbit.js": "^0.4.4",
    "ramda": "^0.25.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "@types/rabbit.js": "^0.4.31",
    "@types/ramda": "^0.25.18",
    "@types/socket.io": "^1.4.31",
    "nodemon": "^1.13.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
