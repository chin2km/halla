{
  "name": "halla-db-server",
  "version": "0.1.0",
  "description": "Halla mongodb server",
  "repository": {
    "type": "git"
  },
  "author": "Chintu Kishen KM",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "watch-node": "nodemon dist/src/index.js",
    "watch-all": "npm-run-all --parallel watch watch-node",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "serve": "node dist/src/index.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "jsonwebtoken": "8.2.0",
    "mongoose": "5.0.7",
    "rabbit.js": "0.4.4",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "@types/bcrypt": "1.0.0",
    "@types/jsonwebtoken": "7.2.6",
    "@types/mongoose": "5.0.2",
    "@types/rabbit.js": "0.4.31",
    "@types/ramda": "0.25.18",
    "nodemon": "1.13.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "5.0.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
